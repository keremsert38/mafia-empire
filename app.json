{
  "expo": {
    "name": "Mafia Empire",
    "slug": "mafia-empire",
    "version": "1.0.0",
    "owner": "kerem.dev",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "android": {
      "package": "com.kerem.boltexponativewind",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "dark",
      "bundleIdentifier": "com.mafiaempire.app",
      "buildNumber": "1",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "itms-apps"
        ],
        "NSCameraUsageDescription": "This app uses the camera to capture profile photos.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photos to let you share images."
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": [
        "applinks:mafiaempire.app"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal"
      },
      "preview": {
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        }
      },
      "production": {
        "android": {
          "buildType": "apk"
        }
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "550536c9-ab28-4bcd-bdd0-4d7c54199254"
      }
    }
  }
}
